{
  "name": "lusine-game-maker",
  "version": "0.2.4",
  "description": "Lusine Game Maker ou LGM3D est un environnement de développement de jeux-vidéo ou applications 3D. Basé sur le concept de la Finite State Machine, Unity, PlayMaker; 3DRad Engine, Scratch, Clickteam Fusion 2.5+.",
  "main": "./out/main/index.js",
  "author": "Cyril5",
  "homepage": "https://github.com/Cyril5/lusine-game-maker",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheckOLD": "npm run typecheck:node && npm run typecheck:web",
    "typecheck": "npm run typecheck:node",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build2": "node --max_old_space_size=4096 ./node_modules/vite/bin/vite.js && npm run build:win",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@babylonjs/havok": "^1.3.1",
    "@babylonjs/materials": "^6.45.1",
    "@blockly/plugin-typed-variable-modal": "^7.0.3",
    "@codemirror/lang-javascript": "^6.1.2",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/remote": "^2.0.9",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@minoru/react-dnd-treeview": "^3.4.4",
    "@types/cannon": "^0.1.8",
    "@uiw/codemirror-theme-darcula": "^4.19.7",
    "@uiw/react-codemirror": "^4.19.7",
    "ammojs": "^0.0.2",
    "ammojs-typed": "^1.0.6",
    "babylon-fbx-loader": "^1.0.9",
    "babylonjs": "^6.46.0",
    "babylonjs-gui": "^6.46.0",
    "babylonjs-inspector": "^6.46.0",
    "babylonjs-loaders": "^6.46.0",
    "babylonjs-materials": "^6.45.1",
    "babylonjs-serializers": "^6.46.0",
    "blockly": "^10.0.0",
    "bootstrap": "^5.2.3",
    "bootswatch": "^5.2.3",
    "cannon": "^0.6.2",
    "class-transformer": "^0.5.1",
    "drawflow": "^0.0.59",
    "electron-log": "^4.4.8",
    "electron-prompt": "^1.7.0",
    "electron-updater": "^5.3.0",
    "immutability-helper": "^3.1.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.7.0",
    "react-router-dom": "^6.4.3",
    "react-treeview": "^0.4.7",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.57.1",
    "terser": "^5.17.1"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.45.1",
    "@babylonjs/inspector": "^6.46.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/drawflow": "^0.0.5",
    "@types/node": "16.18.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.1",
    "electron": "^25.3.1",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.17",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4",
    "vite": "^4.4.6"
  }
}
